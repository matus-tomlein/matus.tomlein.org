{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
		"no-console": 0,
		"react/prop-types": [0, {}],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "react/no-unknown-property": [
            2,
            {
                "ignore": ["class"]
            }
        ]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "modules": true,
            "jsx": true,
            "experimentalObjectRestSpread": true
		}
    },
    "plugins": [
        "react",
        "jsx"
    ]
}
